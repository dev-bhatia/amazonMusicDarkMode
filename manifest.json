{
  "manifest_version": 2,

  "name": "Amazon Music Dark Mode",
  "version": "0.0.1",
  "description": "CSS Magic for the few of us who use AmazonMusic",

  "icons": {
      "16": "icon16.png"
    },

  "content_scripts": [{
    "css": ["dev.css"],
    "run_at": "document_start",
    "matches": ["https://music.amazon.com/*"]
  }]
}
